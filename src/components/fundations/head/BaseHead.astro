---
import Seo from "@/components/fundations/head/Seo.astro";
import Meta from "@/components/fundations/head/Meta.astro";
import Fonts from "@/components/fundations/head/Fonts.astro";
import Favicons from "@/components/fundations/head/Favicons.astro";
import Fuse from "@/components/fundations/scripts/Fuse.astro";
import KeenSlider from "@/components/fundations/scripts/KeenSlider.astro";

const {
  title = "Madrid Landing Kit – Your Children’s First 90 Days in Spain",
  description = "A practical checklist to avoid bureaucracy mistakes when moving to Madrid.",
  canonical,
  image,
} = Astro.props;
---
<Seo
  title={title}
  description={description}
  canonical={canonical}
  image={image}
/>

<Meta />
<Fonts />
<Favicons />

<!-- Scripts -->
<Fuse />
<KeenSlider />

